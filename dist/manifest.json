{
  "manifest_version": 3,
  "name": "AI Chat Manager",
  "version": "1.0.0",
  "description": "Save and organize your AI chats from Gemini, ChatGPT, Claude, and Perplexity into collections.",
  "key": "",
  "permissions": ["identity", "storage", "tabs", "scripting", "activeTab"],
  "host_permissions": ["https://*/*"],
  "oauth2": {
    "client_id": "982276528045-rk3qhp08bmlikfo3qo1nhaojkclq0e4e.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/drive.appdata"]
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["content.js"],
      "matches": ["https://*/*"]
    }
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png"
  }
}
